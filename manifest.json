{
    "manifest_version": 3,
    "name" : "slack-extension",
    "version" : "1.0",
    "description":"個人日報用テンプレート",
    "icons":{
        "16":"./icons/16.png",
        "48":"./icons/48.png",
        "128":"./icons/128.png"
    },
    "permissions":[
        "storage",
        "background",
        "contextMenus",
        "scripting",
        "tabs",
        "activeTab"
    ],
    "background":{
        "service_worker":"js/background.js"
    }

   

}