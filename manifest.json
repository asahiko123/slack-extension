{
    "manifest_version": 3,
    "name" : "Slack-personal-template",
    "version" : "1.0",
    "description":"Slack専用個人日報用テンプレートです。他のサービスでは機能しません。",
    "icons":{
        "16":"./icons/16.png",
        "48":"./icons/48.png",
        "128":"./icons/128.png"
    },
    "permissions":[
        "storage",
        "background",
        "contextMenus",
        "scripting",
        "tabs",
        "activeTab"
    ],
    "background":{
        "service_worker":"js/background.js"
    },
    "action": {
        
        "default_title": "Ext title",
        "default_popup": "popup.html"
    }

   

}